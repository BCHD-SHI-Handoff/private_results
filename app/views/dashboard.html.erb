<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <h1 class="page-header"><span class="glyphicon glyphicon-dashboard"></span> Dashboard</h1>
  <div class="row result-delivery-stats">
    <h2>Results Delivered</h2>

    <div class="col-md-2">
      <h4>Last 30 Days</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:results_delivered][:last_30_days] %>
        </span>
      </h1>
    </div>

    <div class="col-md-2">
      <h4>Last 3 Months</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:results_delivered][:last_3_months] %>
        </span>
      </h1>
    </div>

    <div class="col-md-2">
      <h4>Last 12 Months</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:results_delivered][:last_12_months] %>
        </span>
      </h1>
    </div>

  </div>

  <div class="row results-delivered-stats">
    <h2>Percentage Results Delivered</h2>

    <div class="col-md-2">
      <h4>Last 30 Days</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:percentage_delivered][:last_30_days] %>%
        </span>
      </h1>
    </div>

    <div class="col-md-2">
      <h4>Last 3 Months</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:percentage_delivered][:last_3_months] %>%
        </span>
      </h1>
    </div>

    <div class="col-md-2">
      <h4>Last 12 Months</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:percentage_delivered][:last_12_months] %>%
        </span>
      </h1>
    </div>

  </div>

  <div class="row visit-stats">
    <h2>Visits</h2>

    <div class="col-md-2">
      <h4>Last 30 Days</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:visits][:last_30_days] %>
        </span>
      </h1>
    </div>

    <div class="col-md-2">
      <h4>Last 3 Months</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:visits][:last_3_months] %>
        </span>
      </h1>
    </div>

    <div class="col-md-2">
      <h4>Last 12 Months</h4>
      <h1>
        <span class="label label-primary">
          <%= @dashboard[:visits][:last_12_months] %>
        </span>
      </h1>
    </div>

  </div>
</div>
